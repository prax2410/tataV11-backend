    "scripts": {
        "start": "concurrently \"pm2 start app.js\" \"cd handlers && pm2 start mqttHandler.js\"",
        "stop": "concurrently \"pm2 stop app.js\" \"cd handlers && pm2 stop mqttHandler.js\"",
        "delete-all": "pm2 delete all",
        "watch": "concurrently \"pm2 start app.js --watch\" \"cd handlers && pm2 start mqttHandler.js --watch\"",
        "dev": "concurrently \"nodemon app.js\" \"cd handlers && nodemon mqttHandler.js\"",
        "dev2": "nodemon app.js",
        "logs": "pm2 logs",
        "clear-logs": "pm2 flush"
